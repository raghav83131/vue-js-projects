" v-model=\"selectedSection\">\r\n          <option value=\"\">-Select One-</option>\r\n          <!-- Add options dynamically as needed -->\r\n        </select>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Line Tab -->\r\n    <div v-if=\"currentTab === 'Line'\" class=\"tab-content\">\r\n      <div class=\"input-group\">\r\n        <label for=\"line\">Line:</label>\r\n        <select id=\"line\" v-model=\"selectedLine\" :disabled=\"!selectedSection\">\r\n          <option value=\"\">-ALL-</option>\r\n          <!-- Add options dynamically as needed -->\r\n        </select>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Date Tab -->\r\n    <div v-if=\"currentTab === 'Date'\" class=\"tab-content\">\r\n      <div class=\"input-group\">\r\n        <label for=\"rundate\">Date:</label>\r\n        <select id=\"rundate\" v-model=\"selectedDate\" :disabled=\"!selectedLine\">\r\n          <option value=\"\">-ALL-</option>\r\n          <!-- Add options dynamically as needed -->\r\n        </select>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Km Tab -->\r\n    <div v-if=\"currentTab === 'Km'\" class=\"tab-content\">\r\n      <div class=\"input-group\">\r\n        <label for=\"kmFrom\">Km From:</label>\r\n        <input type=\"number\" id=\"kmFrom\" v-model=\"kmFrom\" min=\"0\" :disabled=\"!selectedSection\" />\r\n      </div>\r\n      <div class=\"input-group\">\r\n        <label for=\"kmTo\">Km To:</label>\r\n        <input type=\"number\" id=\"kmTo\" v-model=\"kmTo\" min=\"0\" :disabled=\"!selectedSection\" />\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Buttons -->\r\n    <div class=\"button-container\">\r\n      <button @click=\"generatePrediction\" class=\"generate-btn\">Generate Prediction</button>\r\n      <button @click=\"resetFilters\" class=\"reset-btn\">Reset Filter</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      currentTab: 'Section', // Default tab\r\n      selectedSection: '',\r\n     